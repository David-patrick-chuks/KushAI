{
    "name": "@kushai/common",
    "version": "0.1.0",
    "description": "Shared utilities and types for Kushai",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": ["dist"],
    "scripts": {
      "build": "tsup src/index.ts --format cjs,esm --dts",
      "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
      "lint": "eslint src --ext .ts",
      "test": "vitest run",
      "clean": "rimraf dist"
    },
    "devDependencies": {
      "rimraf": "^5.0.5",
      "tsup": "^8.0.1",
      "vitest": "^1.0.4"
    }
  }
  